@charset "UTF-8";
@import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&family=Black+Ops+One&display=swap");
@import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap");
* {
  margin: 0;
  border: 0;
  padding: 0;
  transition-timing-function: ease-out;
  transition-duration: 0.2s;
}

body {
  margin: 0;
  background-color: black;
}

header {
  height: 100%;
  width: 100%;
}

button:hover {
  scale: 0.9;
}

body::-webkit-scrollbar {
  width: 15px;
  /* width of the entire scrollbar */
}

body::-webkit-scrollbar-thumb {
  background-color: #d0f44c;
  /* color of the scroll thumb */
  border-radius: 20px;
  /* roundness of the scroll thumb */
  border: 3px solid rgba(245, 245, 245, 0.562);
  /* creates padding around scroll thumb */
}

body::-webkit-scrollbar-thumb:hover {
  background-color: #d0f44c;
  /* color of the scroll thumb */
  scale: 0.9;
  border: 3px solid rgba(119, 117, 117, 0.562);
  /* creates padding around scroll thumb */
}

.promotion {
  background-color: rgba(219, 219, 219, 0.937254902);
  color: black;
  width: 100%;
  height: 4em;
  display: flex;
  align-items: center;
  justify-content: center;
}
.promotion h2 {
  font-size: 36px;
  text-align: center;
}

.logos {
  background-image: url(logos/banner1.jpg);
  border: none;
  background-repeat: repeat-x;
  height: 220px;
  background-position-x: center;
  background-size: cover;
  display: flex;
  justify-content: space-between;
}

#nike-header {
  background-color: rgba(0, 0, 0, 0);
  margin: 10px 10px 10px;
  justify-self: center;
  transition-duration: 0.5s;
  transition-timing-function: ease;
}
#nike-header img {
  height: 100px;
}
#nike-header:hover {
  scale: 1.1;
}

.logo-header {
  background-color: #f7f7f7;
  display: flex;
  justify-content: end;
}

.logo-header1 {
  background-color: rgba(0, 0, 0, 0);
  margin: 10px;
  height: auto;
}
.logo-header1 img {
  background-color: rgba(0, 0, 0, 0);
  height: 40px;
}
.logo-header1:hover {
  scale: 1.2;
}

.menu-principal {
  background-color: #d0f44c;
  width: 100%;
  font-family: "Sawarabi Gothic", Helvetica, sans-serif;
  display: flex;
  justify-content: center;
}

.footmenu {
  display: none;
}

nav .dropdown {
  display: none;
}
nav .dropdown button {
  height: 70px;
  background-color: rgba(8, 8, 8, 0);
}
nav .dropdown button i {
  font-size: 3em;
  text-align: center;
  color: black;
}

.dropdown-menu {
  background-color: #d0f44c;
  height: 400px;
  width: 100vw;
}

.dropdown-item {
  background-color: #d0f44c;
  margin: 0;
  height: 70px;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.dropdown-item:hover {
  background-color: rgba(247, 247, 247, 0.3725490196);
}

.horizontalmenu {
  background-color: rgba(0, 0, 0, 0);
  width: 80%;
  font-size: 1.2em;
  margin: 0;
  list-style: none;
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 5em;
}
.horizontalmenu li {
  box-sizing: content-box;
  width: 250px;
  height: 90%;
  display: flex;
  justify-content: center;
  align-items: center;
  transition-duration: 0.2s;
}
.horizontalmenu li:hover {
  scale: 1.1;
  background-color: rgba(247, 247, 247, 0.3725490196);
  border-radius: 10px;
}

a {
  text-decoration: none;
  background-color: rgba(0, 255, 255, 0);
  /* margin: 10px; */
  font-size: 1.5em;
  color: rgb(0, 0, 0);
}

.banner video {
  width: 100%;
}

main {
  box-sizing: border-box;
  width: 100%;
}

.main-container {
