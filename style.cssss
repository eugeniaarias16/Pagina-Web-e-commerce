.up {
  background-color: rgba(3, 3, 3, 0);
  position: fixed;
  right: 2px;
  padding-left: 5px;
  bottom: 150px;
  border-radius: 50%;
  border: none;
  margin: 5px;
  transition-duration: 0.2s;
  transition-timing-function: ease-out;
}
.up img {
  width: 80px;
}
.up img:hover {
  scale: 1.15;
}
.up:hover {
  scale: 1.1;
  box-shadow: #d0f44c 0 0 15px;
  border-radius: 50%;
}

footer {
