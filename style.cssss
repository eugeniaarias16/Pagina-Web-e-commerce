  display: flex;
}
.home .home-section .carruselhome .homecards {
  margin: 20px;
  height: auto;
  width: 25vw;
  object-fit: contain;
  display: flex;
  object-fit: contain;
  justify-content: center;
  overflow: hidden;
  position: relative;
}
.home .home-section .carruselhome .homecards img {
  width: 90%;
  max-height: 90%;
}
.home .home-section .carruselhome .homecards button {
  margin-top: 5px;
  background-color: rgba(213, 219, 191, 0);
  position: absolute;
  align-self: flex-end;
  font-size: 40px;
  font-family: "Bebas Neue", sans-serif;
  color: white;
  text-shadow: 0 0 5px rgba(0, 0, 0, 0.144);
}

