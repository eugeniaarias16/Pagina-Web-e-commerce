/*style CAPSULA.HTML*/
.capsule-collection {
  display: inline-block;
  width: 100%;
  height: 500px;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url(extras/jordanbanner3.jpg);
}

.capsule {
  display: flex;
  padding-right: 60px;
}
.capsule .filterbutton {
  padding: 15px;
  height: max-content;
  display: flex;
  gap: 5px;
}
.capsule .filterbutton button {
  width: 70%;
  height: max-content;
  padding: 5px;
  border-radius: 10px;
  background-color: rgba(192, 218, 99, 0.8470588235);
  border: solid 2px #556420;
  font-size: 24px;
  font-family: "Bebas Neue", sans-serif;
  color: #556420;
  text-shadow: 0 0px 2px rgba(0, 0, 0, 0.308);
  box-shadow: 0 0 5px #131313;
}
.capsule .filterbutton button:hover {
  scale: 0.9;
  background-color: rgba(217, 240, 137, 0.8470588235);
}
.capsule .filtermenu {
  width: 100%;
  height: 60px;
  padding: 5px;
}
.capsule .filtermenu #orderbutton {
  display: flex;
  justify-content: end;
  width: 70%;
}
.capsule .filters, .capsule #collapse-filter {
  padding-top: 50px;
  position: relative;
  max-width: 50%;
  width: 25%;
  height: 100;
  background-image: linear-gradient(to right, #5e5e5e, #787878, #939393, #aeaeae, #cbcbcb);
  box-shadow: 0 0 15px #6c6e65;
}
.capsule .filters section, .capsule #collapse-filter section {
  margin: 30px 0px;
}
.capsule .filters #img-logo, .capsule #collapse-filter #img-logo {
  width: 100%;
  display: flex;
  justify-content: center;
}
.capsule .filters #img-logo img, .capsule #collapse-filter #img-logo img {
  width: 150px;
  background-color: rgba(17, 17, 17, 0);
}
.capsule .filters h3, .capsule #collapse-filter h3 {
  margin: 20px 0;
  text-align: center;
  font-family: "Bebas Neue", sans-serif;
  font-weight: 600;
  font-size: 60px;
  text-shadow: 0 0 5px #ee0909;
}
.capsule .filters h4, .capsule #collapse-filter h4 {
  text-align: center;
  font-family: "Bebas Neue", sans-serif;
  font-size: 40px;
  font-weight: 200;
  margin: 20px 0;
}
.capsule .filters h5, .capsule #collapse-filter h5 {
  width: 100%;
  text-align: center;
  font-family: "Cairo", sans-serif;
  font-size: 36px;
  font-weight: 400;
  margin-top: 20px;
  margin-bottom: 40px;
  background-color: rgba(247, 247, 247, 0.3725490196);
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.384);
}
.capsule .filters input, .capsule #collapse-filter input {
  margin-right: 10px;
}
.capsule .filters label, .capsule #collapse-filter label {
  font-size: 28px;
  font-family: "Cairo", sans-serif;
  font-weight: 400;
}
.capsule .filters .dropdown-menu, .capsule #collapse-filter .dropdown-menu {
  width: 30%;
}
.capsule .filters #style, .capsule #collapse-filter #style {
  inset: 0;
  margin: auto;
  width: 80%;
  display: flex;
  flex-direction: column;
}
.capsule .filters #style .check, .capsule #collapse-filter #style .check {
  height: 50px;
}
.capsule .filters #size, .capsule #collapse-filter #size {
  inset: 0;
  margin: auto;
  min-width: 80%;
  display: flex;
  flex-wrap: wrap;
}
.capsule .filters #size .check, .capsule #collapse-filter #size .check {
  max-width: 100px;
  margin: 10px;
  width: 80px;
}
.capsule .filters #colors, .capsule #collapse-filter #colors {
  inset: 0;
  margin: auto;
  min-width: 80%;
  width: 80%;
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  justify-content: center;
}
.capsule .filters #colors .form-check, .capsule #collapse-filter #colors .form-check {
  padding: 10px;
  min-width: 70px;
  height: 80px;
  text-align: center;
}
.capsule .filters #colors .form-check label, .capsule #collapse-filter #colors .form-check label {
  width: 100%;
  font-size: 20px;
  font-family: "Cairo", sans-serif;
}
.capsule .filters #colors .form-check .form-check-input, .capsule #collapse-filter #colors .form-check .form-check-input {
  float: right;
  max-width: 60px;
  max-height: 60px;
  height: 40px;
  width: 40px;
  border-radius: 50%;
  border: solid 3px #dbdada;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.226);
}
.capsule .filters #price-range, .capsule #collapse-filter #price-range {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.capsule .filters #price-range .field, .capsule #collapse-filter #price-range .field {
  display: flex;
  gap: 5px;
  width: 90%;
  flex-wrap: wrap;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
